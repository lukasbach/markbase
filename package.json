{
  "name": "document-fast",
  "version": "0.0.0",
  "private": true,
  "description": "Quickly generate an HTML page from a bunch of markdown files",
  "tags": [
    "markdown",
    "documentation",
    "generate",
    "html",
    "homepage",
    "docs",
    "website",
    "fast",
    "cli",
    "tool"
  ],
  "author": "Lukas Bach <contact@lukasbach.com>",
  "license": "MIT",
  "type": "commonjs",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/**/*",
    "!lib/**/*.spec.js",
    "src/**/*"
  ],
  "devDependencies": {
    "@lukasbach/eslint-config-deps": "^1.0.3",
    "eslint": "^8.55.0",
    "typescript": "latest"
  },
  "eslintConfig": {
    "extends": "@lukasbach/base",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "ignorePatterns": [
      "lib",
      "*.js"
    ]
  }
}
